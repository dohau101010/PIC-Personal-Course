.................... //ham quet ma tran phim va chong doi tich cuc khi nhan phim 
.................... //cach 1              
.................... //xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx                         
.................... unsigned int key_4x4_dw() 
.................... {      
....................    unsigned int8 mp1, mp2;   
....................    mp1=key_nhan();       
....................    if(mp1!=0xff)                 
....................    {   
....................       delay_ms(20);                                            
....................       mp1=key_nhan(); 
....................       do               
....................       {          
....................          delay_ms(5);  
....................          mp2=key_nhan();      
....................       }                     
....................       while(mp2==mp1);  
....................    }              
....................    return(mp1); 
.................... }  
