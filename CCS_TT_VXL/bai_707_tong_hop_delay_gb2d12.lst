.................... void so_sanh_nd_dk_buzzer(usi16 nd1, usi16 nd2, int8 sel) 
.................... { 
....................    if(sel==1)        so_sanh_1nd(nd1); 
....................    else if(sel==2)   so_sanh_and_2nd(nd1,nd2); 
....................    else if(sel==3)   so_sanh_or_2nd(nd1,nd2); 
.................... } 
