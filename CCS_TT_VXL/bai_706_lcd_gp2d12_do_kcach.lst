.................... unsigned int8 dht11_read()         
.................... {                                                                                
....................    unsigned int8 kq,j;     
....................    for(j=0;j<8;j++)                                                
....................    {                                                          
....................       while(!dht11_pin_read());  //doi het muc 0     
....................       delay_us(30); 
....................       if(dht11_pin_read()==1)  
....................       {              
....................          kq = (kq<<1)+1;     
....................          delay_us(40);           //bit 1 thi them delay 
....................       }                           
....................       else kq = kq<<1;   
....................    }     
....................    return kq;  
.................... }   
