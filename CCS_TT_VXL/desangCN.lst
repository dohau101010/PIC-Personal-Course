.................... void lcd_puts(usi8 x, usi8 y, unsigned  char* str)  
.................... {                  
....................    while (*str)  
....................    { 
....................       if(y>=20)  
....................       { 
....................          y = 0;   x++; 
....................       } 
....................       if(x>=4) x = 0; 
....................       lcd_goto_xy(x,y);       
....................       if (*str == '\n')  
....................       {                    
....................          x++; 
....................          lcd_goto_xy(x,y); 
....................       }  
