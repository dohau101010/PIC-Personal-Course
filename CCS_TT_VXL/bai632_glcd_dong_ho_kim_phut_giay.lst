....................    if(mp1!=0xff)    
....................    {                        
....................       if(mp2==0xff)              
....................       {                                                         
....................          delay_ms(20);   //chong doi 
....................          mp1=key_nhan(); 
....................          if(mp1!=0xff)   mp2=mp1; 
....................          return(mp1);    //phim moi                   
....................       }   
....................       else                
....................       {                                                                                                              
....................          do 
....................          {                                                     
....................             mp2=key_nhan(); 
....................             delay_ms(5);  
....................          } while(mp2!=0xff);                                                      
....................          return(mp2);     //thoat khi nha phim           
....................       }                 
....................    }            
....................    else                   
....................    { 
....................       mp2=mp1; 
....................       return(mp1);     //thoat vi khong nhan   
....................    }       
.................... } 
