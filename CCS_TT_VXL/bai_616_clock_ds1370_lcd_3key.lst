....................    stepmotor_i++; 
....................    stepmotor_i = stepmotor_i & 0x07; 
.................... }                                                
.................... void step_motor_quay_nghich_hs() 
.................... {      
....................    rbdc=rbdc & 0xffe0; 
....................    rbdc=rbdc|stepmotor_hs[stepmotor_i]; 
....................    xuat_buzzer_relay(); 
....................    stepmotor_i--; 
....................    stepmotor_i = stepmotor_i & 0x07; 
.................... } 
.................... void step_motor_run_tn_hs() 
