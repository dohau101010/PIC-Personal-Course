.................... }                                                                                      
....................  
.................... void dc_motor_quay_nghich() 
.................... { 
....................    output_low(pin_c1);                
....................    output_high(pin_c2); 
.................... } 
....................  
.................... void dc_motor_stop()                                                       
.................... { 
....................    output_low(pin_c1); 
....................    output_low(pin_c2);   
.................... }  
....................  
.................... void dc_motor_run() 
.................... { 
....................    if(dcmotor_onoff) 
....................    { 
....................       if(dcmotor_tn) dc_motor_quay_thuan(); 
....................       else           dc_motor_quay_nghich(); 
....................    } 
....................    else              dc_motor_stop();       
.................... }          
