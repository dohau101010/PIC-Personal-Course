.................... unsigned int key_4x4_up() 
.................... {                     
....................    unsigned int8 mp1,mp2;   
....................    mp1=key_nhan(); 
....................    if(mp1!=0xff)    
....................    {                        
....................       if(mp2==0xff)              
....................       {                                                         
....................          delay_ms(20);   //chong doi 
