.................... {                                                                                
....................    unsigned int8 kq,j;     
....................    for(j=0;j<8;j++)                                                
....................    {                                                          
....................       while(!dht11_pin_read());  //doi het muc 0     
....................       delay_us(30); 
....................       if(dht11_pin_read()==1)  
