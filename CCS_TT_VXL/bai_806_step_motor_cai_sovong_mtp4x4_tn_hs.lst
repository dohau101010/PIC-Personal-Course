.................... float h701_adc_read(usi8 solan,chan_i)                       
.................... { 
....................    float kq_adc; 
....................    set_adc_channel(chan_i);                
....................    delay_us(20);                                           
....................    kq_adc = 0;              
....................    for(int8 i=0;i<solan;i++)   
