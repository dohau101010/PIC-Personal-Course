....................       if(buz_ena)   buzzer_on();                                    
....................       triac_2_off(); //triac 2 dieu khien den gia nhiet 
....................    }                                                                            
....................    else if((nd1<nd_tren)&&(nd2<nd_tren)&&(ttqn==1)) 
