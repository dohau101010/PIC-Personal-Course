....................    unsigned int8 maphim,hang,cot;       
....................    maphim=hang=0xff; 
....................    for(cot=0;cot<4;cot++)                     
....................    {    
....................       output_b(maquet[cot]); 
....................       if     (!input(pin_b2)) {hang=3; break;} 
