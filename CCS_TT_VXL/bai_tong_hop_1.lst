.................... void glcd_xuat_anh(int width,int height,int x,int y)  
.................... {                        
....................    unsigned int i=0, j=0, k=0;                                                    
....................    unsigned int16 n=0;  
....................    for(j=0;j<height;j++)                                      
....................    {     
....................       i=0; 
....................       do 
....................       {                                 
....................          for(k=8;k>0;k--) 
....................          {                   
....................             glcd_pixel(x+i,y+j,bit_test(logo_spk[n],(k-1)));  
....................             i++;                       
....................          }                  
....................          n++;            
....................       }while(i<width);   
....................    }  
.................... } 
